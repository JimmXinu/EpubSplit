msgstr "%s jako"

#: epubsplit_plugin.py:360
msgid "Calibre Library Link"
msgstr "Calibre-kirjaston linkki"

#: epubsplit_plugin.py:365
msgid "Source URL"
msgstr "Lähde-URL"

#: epubsplit_plugin.py:374
