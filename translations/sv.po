msgid "Section Sample"
msgstr "Sektionsprov"

#: dialogs.py:320
msgid "Check Selected"
msgstr "Kryssa i markerad"

#: dialogs.py:321
msgid "Uncheck Selected"
msgstr "Kryssa ur markerad"

