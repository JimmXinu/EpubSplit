msgid "%d day"
msgstr "%d день"

#: dialogs.py:420
msgid "%d days"
msgstr "%d дні"

#: dialogs.py:421
msgid "%d hour"
msgstr "%d година"

#: dialogs.py:421
msgid "%d hours"
msgstr "%d години"

#: dialogs.py:422
msgid "%d minute"
msgstr "%d хвилина"

#: dialogs.py:422
msgid "%d minutes"
msgstr "%d хвилин"

