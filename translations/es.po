msgstr "EpubSplit"

#: epubsplit_plugin.py:69
msgid "Split off part of an EPUB into a new book."
msgstr "Separar parte de un EPUB en un nuevo libro."

#: epubsplit_plugin.py:117
msgid "Select One Book"
msgstr "Seleccione un libro"

#: epubsplit_plugin.py:118
msgid "Please select exactly one book to split."
msgstr "Seleccione sólo un libro para dividir."

#: epubsplit_plugin.py:138
msgid "EPUB3 Detected"
msgstr "EPUB3 detectado"

#: epubsplit_plugin.py:139
msgid "This plugin only works on EPUB2 format ebooks."
msgstr "Este complemento funciona sólo con libros en el formato EPUB2."

#: epubsplit_plugin.py:143
msgid "No EPUB"
msgstr "No es EPUB"

#: epubsplit_plugin.py:144
msgid "This plugin only works on EPUB format ebooks."
msgstr "Este complemento funciona sólo con libros en el formato EPUB."

#: epubsplit_plugin.py:155
msgid "Select Sections to Split Off"
msgstr "Seleccione secciones para dividir"

#: epubsplit_plugin.py:174
msgid "No Sections Selected"
msgstr "No hay secciones seleccionadas"

