msgid "Section Sample"
msgstr "Przykład sekcji"

#: dialogs.py:320
msgid "Check Selected"
msgstr "Zaznacz wybrane"

#: dialogs.py:321
msgid "Uncheck Selected"
msgstr "Odznacz wybrane"

